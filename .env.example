# Bot token from @BotFather
BOT_TOKEN=your_bot_token_here

# Server pepper - generate with: openssl rand -hex 32
SERVER_PEPPER=generate_a_secure_random_pepper_here

# Admin Telegram user IDs (comma separated)
ADMIN_IDS=123456789,987654321

# Session timeout in seconds (default: 600 = 10 minutes)
SESSION_TIMEOUT=600

# Database path
DB_PATH=./vaultbot.db

# Log level (DEBUG, INFO, WARNING, ERROR, CRITICAL)
LOG_LEVEL=INFO
